               </widget>
              </item>
              <item row="0" column="1">
               <widget class="QSpinBox" name="compiledPageCacheSizeEdit">
                <property name="buttonSymbols">
                 <enum>QAbstractSpinBox::PlusMinus</enum>
                </property>
                <property name="suffix">
                 <string> kB</string>
                </property>
                <property name="minimum">
                 <number>65536</number>
                </property>
                <property name="maximum">
                 <number>1048576</number>
                </property>
